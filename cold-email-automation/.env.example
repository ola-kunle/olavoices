# API Keys for Cold Email Automation
# Copy this file to .env and fill in your actual keys

# Apollo.io API Key (Free tier: 50 credits/month)
# Get it at: https://app.apollo.io/#/settings/integrations/api
APOLLO_API_KEY=your_apollo_api_key_here

# Hunter.io API Key (Free tier: 50 searches/month)
# Get it at: https://hunter.io/api-keys
HUNTER_API_KEY=your_hunter_api_key_here

# Google Gemini API Key (Free tier: 1,500 requests/day)
# Get it at: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Gmail API Credentials
# Follow setup guide to get these
GMAIL_CLIENT_ID=your_gmail_client_id
GMAIL_CLIENT_SECRET=your_gmail_client_secret
GMAIL_REFRESH_TOKEN=your_gmail_refresh_token

# Sender Configuration
FROM_EMAIL=hello@olavoices.com
FROM_NAME=Ola

# Daily Limits (for warm-up and safety)
DAILY_EMAIL_LIMIT=10
DAILY_LEAD_LIMIT=3

# Automation Settings
DRY_RUN=false
DEBUG=false
